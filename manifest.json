{
    "name": "Connect",
    "description": "Connect in single tap.",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        
      "default_popup":"popup.html",
        "default_icon": "logo.png"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["script.js"]
        }
      ],
    "permissions": ["activeTab"]
}